{% extends "RmzamoraCalendarBundle::layout.html.twig" %}
{% block rmzamora_calendar_content %}
<form action="{{ path(rmzamora_calendar_route('calendar', 'add')) }}" {{ form_enctype(form) }} method="POST">
    {{ form_errors(form) }}

    {{ form_row(form.name) }}

    {{ form_rest(form) }}

    <input type="submit" value="Create Calendar" />
</form>
{% endblock rmzamora_calendar_content %}
